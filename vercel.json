{
  "version": 2,
  "builds": [
    { "src": "dist/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/creator", "dest": "/dist/creator.html" },
    { "src": "/creator/(.*)", "dest": "/dist/creator.html" },

    { "src": "/consumer", "dest": "/dist/consumer.html" },
    { "src": "/consumer/(.*)", "dest": "/dist/consumer.html" },

    { "src": "/photo", "dest": "/dist/photo.html" },
    { "src": "/photo/(.*)", "dest": "/dist/photo.html" },

    { "src": "/(.*)", "dest": "/dist/$1" },
    { "src": "/.*", "dest": "/dist/index.html" }
  ]
}
